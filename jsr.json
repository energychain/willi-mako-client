{
  "name": "@energychain/willi-mako-client",
  "version": "0.3.6",
  "description": "Official TypeScript client, CLI, and demo tooling for orchestrating Willi-Mako Marktkommunikation APIs.",
  "compatibility": {
    "deno": ">=1.42",
    "node": ">=18",
    "bun": ">=1.0"
  },
  "exports": {
    ".": "./src/index.ts",
    "./cli": "./src/cli.ts",
    "./types": "./src/types.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "schemas/openapi.json",
      "README.md",
      "LICENSE"
    ],
    "exclude": [
      "**/*.test.ts",
      "tests/**"
    ]
  }
}
